{
  "name": "shadcn-vue",
  "type": "module",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "bunx --bun vite",
    "build": "bunx --bun vue-tsc -b && bunx --bun vite build",
    "preview": "bunx --bun vite preview",
    "lint": "bunx --bun eslint",
    "lint:staged": "bunx --bun eslint --pass-on-no-patterns --no-warn-ignored $(git diff --cached --name-only --diff-filter=d)",
    "lint:fix": "bunx --bun eslint --fix",
    "lint:fix-staged": "bunx --bun eslint --fix --pass-on-no-patterns --no-warn-ignored $(git diff --cached --name-only --diff-filter=d)",
    "lint:inspect": "bunx --bun eslint --inspect-config",
    "precommit": "bun lint:staged"
  },
  "dependencies": {
    "@iconify/vue": "^5.0.0",
    "@internationalized/date": "^3.8.2",
    "@tailwindcss/vite": "^4.1.11",
    "@tanstack/vue-table": "^8.21.3",
    "@unovis/vue": "^1.5.2",
    "@vee-validate/zod": "^4.15.1",
    "@vueuse/components": "^13.4.0",
    "@vueuse/core": "^13.4.0",
    "@vueuse/integrations": "^13.4.0",
    "@vueuse/router": "^13.4.0",
    "change-case": "^5.4.4",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "embla-carousel-vue": "^8.6.0",
    "lucide-vue-next": "^0.513.0",
    "non.geist": "^1.0.4",
    "nprogress": "^0.2.0",
    "pinia": "^3.0.3",
    "reka-ui": "^2.3.1",
    "simple-git-hooks": "^2.13.0",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4.1.11",
    "tw-animate-css": "^1.3.4",
    "vaul-vue": "^0.4.1",
    "vee-validate": "^4.15.1",
    "vue": "^3.5.17",
    "vue-router": "^4.5.1",
    "vue-sonner": "^2.0.1"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.16.1",
    "@types/common-tags": "^1.8.4",
    "@types/node": "^22.15.34",
    "@vitejs/plugin-vue": "^5.2.4",
    "@vue/tsconfig": "^0.7.0",
    "common-tags": "^1.8.2",
    "eslint": "^9.30.0",
    "eslint-plugin-better-tailwindcss": "^3.4.2",
    "eslint-plugin-format": "^1.0.1",
    "typescript": "~5.8.3",
    "vite": "^6.3.5",
    "vite-svg-loader": "^5.1.0",
    "vue-tsc": "^2.2.10"
  },
  "simple-git-hooks": {
    "pre-commit": "bun precommit"
  }
}
